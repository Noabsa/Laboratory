{
  "name": "goodo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precdk-mainStack": "npm ci",
    "cdk-mainStack": "cd ./backend && npm ci && npx cdk deploy --require-approval never gooDo-$AWS_STAGE-mainStack",
    "test-app": "npx jest --projects ./frontend",
    "test-server": "npx jest --projects ./backend",
    "local-server": "cd ./backend && npm run server",
    "local-app": "set REACT_APP_STAGE=local && cd ./frontend && npm start",
    "dev-app": "set REACT_APP_STAGE=dev && cd ./frontend && npm start",
    "setup": "npm i && cd ./backend && npm i && cd ../frontend && npm i"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/node": "^20.8.3",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.5"
  }
}
